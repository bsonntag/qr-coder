{
  "name": "react-app-template",
  "version": "0.0.0",
  "description": "Just a template that I use for React web apps.",
  "scripts": {
    "build": "gulp assets",
    "lint": "eslint --ext .js --ext .jsx .",
    "spec": "mocha --compilers jsx:babel-register --recursive -R spec",
    "start": "gulp",
    "test": "npm run spec && npm run lint"
  },
  "keywords": [
